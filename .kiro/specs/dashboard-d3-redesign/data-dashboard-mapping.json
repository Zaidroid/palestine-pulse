      "output": "TimeSeriesData[]",
      "parameters": ["timeRange", "aggregationPeriod"]
    },
    "aggregateByFacilityType": {
      "description": "Group healthcare attacks by facility type",
      "input": "HealthcareAttackRecord[]",
      "output": "CategoryData[]"
    },
    "groupByCategory": {
      "description": "Generic category grouping with count",
      "input": "Array<{category: string, ...}>",
      "output": "CategoryData[]",
      "parameters": ["categoryField", "valueField"]
    },
    "transformToPyramid": {
      "description": "Transform demographic data to pyramid structure",
      "input": "Array<{age: number, gender: string}>",
      "output": "PyramidData[]",
      "parameters": ["ageGroups"]
    },
    "transformToCalendar": {
      "description": "Transform daily data to calendar heatmap format",
      "input": "Array<{date: string, value: number}>",
      "output": "CalendarData[]"
    },
    "multiMetricTimeSeries": {
      "description": "Combine multiple World Bank indicators into time series",
      "input": "WorldBankDataPoint[]",
      "output": "TimeSeriesData[]",
      "parameters": ["indicators"]
    },
    "timeSeriesByGender": {
      "description": "Create separate time series for male/female/total",
      "input": "WorldBankDataPoint[]",
      "output": "{total: TimeSeriesData[], male: TimeSeriesData[], female: TimeSeriesData[]}"
    },
    "latestValuesByIndicator": {
      "description": "Get latest value for each indicator",
      "input": "WorldBankDataPoint[]",
